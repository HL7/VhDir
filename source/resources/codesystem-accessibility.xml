	<content value="complete"/>
	<concept>
		<code value="cultcomp"/>
		<display value="Cultural competence"/>
		<definition value="Provides services that meet the social,cultural, and linguistic needs of persons."/>
	</concept>
	<concept>
		<code value="handiaccess"/>
		<display value="handicap accessible"/>
		<definition value="Provides access to persons with disabilities."/>
	</concept>
	<concept>
		<code value="adacomp"/>
		<display value="ADA compliant"/>
		<definition value="Provides access which complies with the Americans with Disabilities Act and/or amendments."/>
	</concept>
	<concept>
		<code value="pubtrans"/>
		<display value="public transit options"/>
		<definition value="Provides access using public transportation modes."/>
	</concept>
	<concept>
		<code value="anssrvc"/>
		<display value="answering service"/>
		<definition value="Provides capability to communicate when intended recipient is not immediately available."/>
	</concept>
	<concept>
		<code value="vision"/>
		<display value="vision"/>
		<definition value="Provides services for visually impaired persons."/>
	</concept>
	<concept>
		<code value="cognitive"/>
		<display value="cognitive"/>
		<definition value="Provides services for cognitively impaired persons."/>
	</concept>
	<concept>
		<code value="mobility"/>
		<display value="mobility"/>
		<definition value="Provides services for mobility impaired persons."/>
	</concept>
